    "/api/dailynutritions/:{mealId}": {
      "delete": {
        "tags": [
          "DELETE"
        ],
        "summary": "Delete purchase product by ID",
        "description": "",
        "operationId": "deleteProduct",
        "parameters": [
          {
            "name": "Bearer Token",
            "in": "query",
