                "schema": {
                  "$ref": "#/components/schemas/AddMealResp"
                }
              },
              "application/xml": {
                "schema": {
                  "$ref": "#/components/schemas/AddMealResp"
                }
              }
            }
          },
          "400": {
            "description": "Invalid meal name supplied"
          },
          "404": {
            "description": "Product not found"
          }
        }
